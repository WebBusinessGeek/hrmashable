$('#left_side_blogs').append("<%= j render '/blogs/left_blogs', :blogs => @left_blogs %>")
$('#center_blogs').append("<%= j render '/blogs/center_blogs', :blogs => @center_blogs %>")
$('.right_side_blogs').append("<%= j render '/blogs/right_blogs', :blogs => @right_blogs %>")

<% if @left_blogs.next_page %>
  $('#left_side_blogs .pagination').replaceWith('<%= j will_paginate(@left_blogs) %>');
<% else %>
  $('#left_side_blogs .pagination').remove();
<% end %>

<% if @center_blogs.next_page %>
  $('#center_blogs .pagination').replaceWith('<%= j will_paginate(@center_blogs) %>');
<% else %>
  $('#center_blogs .pagination').remove();
<% end %>

<% if @right_blogs.next_page %>
  $('.right_side_blogs .pagination').replaceWith('<%= j will_paginate(@right_blogs) %>');
<% else %>
  $('.right_side_blogs .pagination').remove();
<% end %>
